"class": "Door",
"language": "GDScript",
"path": "res://props/door/Door.gd"
}, {
"base": "KinematicBody2D",
"class": "Enemy",
"language": "GDScript",
"path": "res://characters/enemies/Enemy.gd"
}, {
"base": "Node2D",
"class": "Game",
"language": "GDScript",
"path": "res://Game.gd"
}, {
"base": "StaticBody2D",
"class": "Pickup",
"language": "GDScript",
"path": "res://props/pickup/Pickup.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
